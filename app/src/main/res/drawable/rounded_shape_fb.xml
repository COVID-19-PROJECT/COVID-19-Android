<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"  android:layout_height="60dp" android:layout_width="60dp">
    <item>
        <shape android:shape="rectangle">
            <padding
                android:left="0dp"
                android:right="0dp"
                android:top="0dp"
                android:bottom="0dp"
                />
            <gradient
                android:angle="90"
                android:endColor="@color/cardview_shadow_end_color"
                android:startColor="@color/cardview_shadow_start_color" />
            <corners
                android:radius="80dp"/>
        </shape>
    </item>
    <item android:drawable="@drawable/f_logo_blue_58"></item>
</layer-list>